{
  "name": "kdoc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev --turbopack",
    "dev:verbose": "DEBUG=* next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "seed": "prisma db seed",
    "postinstall": "prisma generate",
    "type-check": "tsc --noEmit --watch",
    "check": "pnpm lint && pnpm type-check",
    "find-unused": "tsx scripts/find-unused-ts-api.ts",
    "clean-unused": "tsx scripts/find-unused-files.ts --delete",
    "analyze-unused": "tsx scripts/find-unused-files.ts --json"
  },
  "dependencies": {
    "@notionhq/client": "^4.0.1",
    "@prisma/client": "^6.13.0",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.55.0",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.12",
    "gsap": "^3.13.0",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "konva": "^9.3.22",
    "lucide-react": "^0.539.0",
    "motion": "^12.23.12",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-konva": "^19.0.7",
    "server-only": "^0.0.1",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "glob": "^11.0.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.13.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
